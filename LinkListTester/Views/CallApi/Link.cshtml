@model IEnumerable<Link>
@{ ViewBag.Title = "All Links";}

<h2>All Reservations</h2>
<table class="table table-sm table-striped table-bordered m-2">
    <thead><tr><th>ID</th><th>Uri</th><th>Category</th></tr></thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Id</td>
                <td>@r.Uri</td>
                <td>@r.Category</td>
                
            </tr>
        }
    </tbody>
</table>
